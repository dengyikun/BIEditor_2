{
  "entry": "src/index.js",
  "publicPath": ".",
  "outputPath": "../build/render",
  "extraBabelPlugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    "transform-runtime"
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  }
}
